        <div class="item">
          <!-- Item image -->
          <div class="item-image">
            <a href="single-item.html">
             {% image '@AppSiteBundle/Resources/public/img/photos/2.png' %}
             <img src="{{ asset_url }}" alt="Example" />
             {% endimage %}
           </a>
         </div>
         <!-- Item details -->
         <div class="item-details">
